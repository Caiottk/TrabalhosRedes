[Unit]
Description=Mount unit for intellij-idea-educational, revision 51
After=snapd.mounts-pre.target
Before=snapd.mounts.target
Before=local-fs.target

[Mount]
What=/var/lib/snapd/snaps/intellij-idea-educational_51.snap
Where=/snap/intellij-idea-educational/51
Type=squashfs
Options=nodev,ro,x-gdu.hide,x-gvfs-hide
LazyUnmount=yes

[Install]
WantedBy=snapd.mounts.target
WantedBy=multi-user.target
